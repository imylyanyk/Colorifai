{
"manifest_version": 2,
    "name": "Colorifai",
    "description": "Helps colorblind people browse the internet in colors",
    "version": "1",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
				"credentials.js",
				"jquery-1.12.0.min.js",
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}